<%# 页面 %>

<%# 描述这个文件用于展示一些自定义的页面 %>
<%# 这个文件不接收参数 %>

<%- partial("partial/topbar", {active: page.title}) %>
<main>
    <div class="container">
        <h1 class = "page-title">
            <% if (page.title == 'search') { %>
            <%- _p('search') %>
            <% } else {%>
            <%= theme.menu[page.title].title %>
            <% } %>
        </h1>
        <% if (page.title) { %>
            <%- partial("page/" + page.title); %>
        <% } else { %>
            <%- partial("page/default"); %>
        <% } %>
    </div>
</main>
